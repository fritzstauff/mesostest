{
  "id": "apptest",
  "cmd": null,
  "cpus": 0.25,
  "disk": 0,
  "instances": 2,
  "ports": [
    0
  ],
  "networks": [ { "mode": "container/bridge" } ],
  "container": {
    "docker": {
      "image": "portus.stacklocity.net/hivelocity/whoami",
      "forcePullImage": true
    },
    "portMappings": [
      {
         "containerPort": 80,
         "hostPort": 0,
         "labels": {
            "VIP_0": "apptest:10000"
         },
         "protocol": "tcp",
         "servicePort": 10000,
         "name": "apptest"
      }
    ],
    "type": "MESOS",
    "volumes": []
  },
  "env": {},
  "healthChecks": [{
    "portIndex": 0,
    "protocol": "TCP",
    "gracePeriodSeconds": 300,
    "intervalSeconds": 60,
    "timeoutSeconds": 20,
    "maxConsecutiveFailures": 0
  }],
  "labels": {
    "HAPROXY_GROUP": "external"
  },
  "uris": [
    "file:///etc/docker.tar.gz"
  ]
}

{
  "id": "apptest",
  "cmd": null,
  "cpus": 0,
  "disk": 0,
  "instances": 2,
  "ports": [
    0
  ],
  "container": {
    "docker": {
      "network": "BRIDGE",
      "image": "portus.stacklocity.net/hivelocity/whoami",
      "forcePullImage": true,
      "portMappings": [
        {
          "protocol": "tcp",
          "name": null,
          "containerPort": 80,
          "hostPort": 80,
          "labels": {
            "VIP_0": "/apptest:80"
          }
        }
      ]
    },
    "type": "DOCKER",
    "volumes": []
  },
  "env": {},
  "healthChecks": [{
    "portIndex": 0,
    "protocol": "TCP",
    "gracePeriodSeconds": 300,
    "intervalSeconds": 60,
    "timeoutSeconds": 20,
    "maxConsecutiveFailures": 0
  }],
  "labels": {
    "HAPROXY_0_MODE": "http",
    "HAPROXY_GROUP": "internal"
  },
  "uris": [
    "file:///etc/docker.tar.gz"
  ]
}
